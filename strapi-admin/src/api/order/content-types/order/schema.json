{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": "Customer orders"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "pincode": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "totalOrderValue": {
      "type": "integer",
      "required": true
    },
    "userid": {
      "type": "biginteger"
    },
    "city": {
      "type": "string"
    },
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "DeliveryStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "On the Way",
        "Delivered"
      ],
      "default": "Pending"
    },
    "vendor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendor.vendor",
      "inversedBy": "orders"
    }
  }
}
